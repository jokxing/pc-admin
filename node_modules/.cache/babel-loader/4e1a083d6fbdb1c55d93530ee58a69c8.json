A,GAAA,GAAA,GAAA,GAAA;AACA,KAxBA;AAyBA;AACA,IAAA,KAAA,EAAA,iBAAA;AACA,UAAA,KAAA,GAAA,IAAA,IAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,EAAA,OAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA,IAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,EAAA,OAAA,EAAA;AACA,UAAA,KAAA,GAAA,KAAA,GAAA,KAAA;AACA,UAAA,IAAA,GAAA,KAAA,IAAA,KAAA,EAAA,GAAA,EAAA,GAAA,IAAA,CAAA;;AAEA,UAAA,IAAA,GAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,eAAA;AACA;AACA,KAnCA;AAoCA;AACA,IAAA,cAAA,EAAA,wBAAA,EAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,kBADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,EAAA,EAAA;AADA;AAFA,OAAA;AAMA,KA5CA;AA6CA;AACA,IAAA,mBA9CA,+BA8CA,GA9CA,EA8CA;AACA,WAAA,IAAA,GAAA,GAAA;AACA;AAhDA,GA3DA;AA6GA,EAAA,OA7GA,qBA6GA;AACA,QAAA,KAAA,GAAA,IAAA;;AACA,IAAA,MAAA,CAAA,IAAA,CAAA,aAAA,CAAA,IAAA,EAAA,OAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,aAAA,CAAA,IAAA,CAAA,CAAA,EAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,aAAA,CAAA,IAAA,CAAA,CAAA,EAAA,CAAA,CAAA;AACA;AACA,KAJA;AAKA;AApHA,CAAA","sourcesContent":["<template>\r\n    <div class=\"gameRecord\">\r\n        <h3 class=\"title\">游戏记录</h3>\r\n\r\n        <el-row :gutter=\"6\" class=\"head\">\r\n            <el-form>\r\n                <el-form-item style=\"margin-bottom: 10px\">\r\n                    <el-col :span=\"7\">\r\n                        <el-input placeholder=\"会员账号\" v-model=\"name\"></el-input>\r\n                    </el-col>\r\n                    <el-col :span=\"5\">\r\n                        <el-select placeholder=\"请选择游戏平台\" v-model=\"gameCode\">\r\n                            <el-option v-for=\"(item, index) in game\" :key=\"index\" :label=\"item.gameTitle\" :value=\"item.gameCode\"></el-option>\r\n                        </el-select>\r\n                    </el-col>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-col :span=\"7\">\r\n                        <el-date-picker\r\n                            v-model=\"date\"\r\n                            type=\"daterange\"\r\n                            :clearable=\"false\"\r\n                            :picker-options=\"pickerOptions\"\r\n                            range-separator=\"至\"\r\n                            value-format=\"yyyy-MM-dd\"\r\n                            start-placeholder=\"开始日期\"\r\n                            end-placeholder=\"结束日期\">\r\n                        </el-date-picker>\r\n                    </el-col>\r\n                    <el-col :span=\"8\">\r\n                         <el-radio-group v-model=\"tabPosition\" @change=\"tabChange\">\r\n                            <el-radio-button label=\"1\">今日</el-radio-button>\r\n                            <el-radio-button label=\"2\">昨日</el-radio-button>\r\n                            <el-radio-button label=\"3\">7天</el-radio-button>\r\n                            <el-radio-button label=\"4\">30天</el-radio-button>\r\n                        </el-radio-group>\r\n                    </el-col>\r\n                    <el-col :span=\"8\">\r\n                        <el-button round class=\"primary-button\" @click=\"query()\">查询</el-button>\r\n                        <el-button round plain>重置</el-button>\r\n                    </el-col>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-row>\r\n\r\n        <!-- 表格 -->\r\n        <el-row>\r\n            <el-table\r\n                :data=\"tableData\"\r\n                style=\"width: 100%\">\r\n                <el-table-column\r\n                    label=\"比赛类型\"\r\n                    width=\"120\">\r\n                    <template slot-scope=\"scope\">\r\n                        <span class=\"match_type primary-color\" @click=\"matchTypeClick(scope.row.id)\">{{ scope.row.type }}</span>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                    label=\"注单笔数\"\r\n                    prop=\"num\"\r\n                    width=\"150\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    label=\"投注金额\"\r\n                    prop=\"money\"\r\n                    width=\"150\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    label=\"会员\"\r\n                    align=\"center\">\r\n                    <el-table-column label=\"返水\" prop=\"backwater\" width=\"100\" align=\"center\"></el-table-column>\r\n                    <el-table-column label=\"结束\" prop=\"end\" align=\"center\"></el-table-column>\r\n                </el-table-column>\r\n                <el-table-column\r\n                    label=\"有效投注\"\r\n                    prop=\"beat\"\r\n                    width=\"150\">\r\n                </el-table-column>\r\n            </el-table>\r\n        </el-row>\r\n\r\n        <!-- 分页 -->\r\n        <el-pagination\r\n            background\r\n            layout=\"prev, pager, next\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page.sync=\"page\" \r\n            :total=\"total\">\r\n        </el-pagination>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import gameConfigure from '../../assets/js/gameConfigure.js'\r\n    export default {\r\n        data() {\r\n            var myDate = new Date();\r\n            return {\r\n                // 查询\r\n                date: [myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\", myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate()],\r\n                pickerOptions: {\r\n                    disabledDate: (time) => {\r\n                        return time.getTime() < (myDate.getTime() - 24*60*60*1000*31) || time.getTime() >= (Date.now())\r\n                    }\r\n                },\r\n                name: \"\",\r\n                game:[],\r\n                gameCode: \"\",\r\n                tabPosition: '-1',\r\n                // 表格\r\n                tableData:[\r\n                    {\r\n                        id: 1,\r\n                        type: '体育',\r\n                        num: 111,\r\n                        money: \"10.00\",\r\n                        backwater: \"10.00\",\r\n                        end: '1000.00',\r\n                        beat: \"10000.00\"\r\n                    },\r\n                    {\r\n                        id: 1,\r\n                        type: '体育',\r\n                        num: 111,\r\n                        money: \"10.00\",\r\n                        backwater: \"10.00\",\r\n                        end: '1000.00',\r\n                        beat: \"10000.00\"\r\n                    },\r\n                    {\r\n                        id: 1,\r\n                        type: '体育',\r\n                        num: 111,\r\n                        money: \"10.00\",\r\n                        backwater: \"10.00\",\r\n                        end: '1000.00',\r\n                        beat: \"10000.00\"\r\n                    },\r\n                    {\r\n                        id: 1,\r\n                        type: '体育',\r\n                        num: 111,\r\n                        money: \"10.00\",\r\n                        backwater: \"10.00\",\r\n                        end: '1000.00',\r\n                        beat: \"10000.00\"\r\n                    },\r\n                ],\r\n                // 分页\r\n                page: 1,\r\n                total: 0\r\n            }\r\n        },   \r\n        methods: {\r\n            // 选择时间\r\n            tabChange: function (tab, event) {\r\n                var myDate = new Date();\r\n                if(tab == 1){\r\n                    this.date = [this.getDate(myDate.getTime()), this.getDate(myDate.getTime())]\r\n                }else if(tab == 2){\r\n                    var yesterday = myDate.getTime() - 24*60*60*1000;\r\n                    this.date = [this.getDate(yesterday), this.getDate(myDate.getTime())]\r\n                }else if(tab == 3){\r\n                    var seven = myDate.getTime() - 24*60*60*1000*7;\r\n                    this.date = [this.getDate(seven), this.getDate(myDate.getTime())]\r\n                }else{\r\n                    var thirty = myDate.getTime() - 24*60*60*1000*30;\r\n                    this.date = [this.getDate(thirty), this.getDate(myDate.getTime())]\r\n                }\r\n            },\r\n            // 计算时间戳\r\n            getDate: function (time) {\r\n                var myDate = new Date(time);\r\n                var year = myDate.getFullYear();\r\n                var month = myDate.getMonth() + 1;\r\n                var day = myDate.getDate();\r\n                return year + \"-\" + month + \"-\" + day;\r\n            },\r\n            // 查询\r\n            query: function () {\r\n                var date1 = new Date(this.date[0]).getTime();\r\n                var date2 = new Date(this.date[1]).getTime();\r\n                var dates = date2 - date1;\r\n                var days = dates / (24*60*60*1000)\r\n\r\n                if(days > 30){\r\n                    this.$message.error('只能查询最近30天的时间。');\r\n                }\r\n            },\r\n            // 比赛类型进去详情页面\r\n            matchTypeClick: function (id) {\r\n                this.$router.push({\r\n                    name: 'memberBetDetails',\r\n                    params: {\r\n                        id: id\r\n                    }\r\n                })\r\n            },\r\n            // 分页\r\n            handleCurrentChange(val) {\r\n                this.page = val\r\n            }\r\n        },\r\n        mounted() {\r\n            var _this = this;\r\n            Object.keys(gameConfigure.game).forEach(function(j,k){\r\n                for(var i = 0; i < gameConfigure.game[j].length; i++){\r\n                    _this.game.push(gameConfigure.game[j][i])\r\n                }\r\n            })\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/memberRecord.css);\r\n</style>"],"sourceRoot":"src/views/memberRecord"}]}