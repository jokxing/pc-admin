utton class=\"primary-button\" round @click=\"handleQuery\">查询</el-button>\r\n                        <el-button plain round @click=\"hanldReset\">重置</el-button>\r\n                    </el-col>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-row>\r\n\r\n        <el-row>\r\n            <el-table\r\n                :data=\"tableData\"\r\n                border\r\n                style=\"width: 100%\"\r\n                v-loading=\"showLoading\">\r\n                <el-table-column\r\n                    fixed\r\n                    label=\"代理账号\"\r\n                    width=\"150\">\r\n                    <template slot-scope=\"scope\">\r\n                        <div>\r\n                            <router-link class=\"primary-color\" :to=\"'/payoutReport/payoutDetailed?Start=' + data.Start + '&End=' + data.End\">\r\n                                {{ scope.row.code }} ({{ scope.row.realname }})\r\n                            </router-link>\r\n                        </div>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"beat_num\"\r\n                    label=\"注单笔数\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"beat_money\"\r\n                    label=\"投注金额\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"valid_betamt\"\r\n                    label=\"有效投注\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"payout\"\r\n                    label=\"会员输赢\"\r\n                    width=\"100\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"bonus_money\"\r\n                    label=\"红利\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"agent_bonus\"\r\n                    label=\"代理彩金\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"user_agent\"\r\n                    label=\"邀请有奖\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"uservip_system\"\r\n                    label=\"VIP系统\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"bonus_system\"\r\n                    label=\"彩金系统\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"water_money\"\r\n                    label=\"返水\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"dpt_bonus_money\"\r\n                    label=\"存款优惠\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"game_fee\"\r\n                    label=\"平台费\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"user_num\"\r\n                    label=\"会员数\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"valid_user\"\r\n                    label=\"有效会员数\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"dpt_money\"\r\n                    label=\"存款额度\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"wtd_money\"\r\n                    label=\"提款额度\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"transfer_user\"\r\n                    label=\"会员转账\"\r\n                    width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"fee\"\r\n                    label=\"（汇/取）手续费\"\r\n                    width=\"170\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    label=\"代理输赢\"\r\n                    width=\"120\">\r\n                    <template slot-scope=\"scope\">\r\n                        <span :class=\"{ 'red': scope.row.real_win_lost < 0, 'green': scope.row.real_win_lost > 0 }\">\r\n                            {{ scope.row.real_win_lost }}\r\n                        </span>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n        </el-row>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import { companyReportApi } from '../../http/api'\r\n    var myDate = new Date();\r\n    var year = myDate.getFullYear()\r\n    var month = (myDate.getMonth() + 1) < 10 ? \"0\" + (myDate.getMonth() + 1) : myDate.getMonth() + 1;\r\n    var day = myDate.getDate()\r\n    export default {\r\n        data() {\r\n            return {\r\n                date: [myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\", myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate()],\r\n                pickerOptions: {\r\n                    disabledDate: (time) => {\r\n                        return time.getTime() < (myDate.getTime() - 24*60*60*1000*31*3) || time.getTime() >= (Date.now())\r\n                    }\r\n                },\r\n                data: {\r\n                    Start: year + \"-\" + month + \"-01\",\r\n                    End: year + \"-\" + month + \"-\" + day,\r\n                },\r\n\r\n                tableData: [],\r\n                showLoading: false\r\n            }\r\n        },\r\n        methods: {\r\n            payoutReportData () {\r\n                var _this = this;\r\n                this.showLoading = true;\r\n                companyReportApi(_this.data).then(res => {\r\n                    _this.showLoading = false;\r\n                    _this.tableData = res.data.message\r\n                }).catch(err => {\r\n                    _this.showLoading = false\r\n                })\r\n            },\r\n            handleQuery() {\r\n                this.Start = this.date[0];\r\n                this.End = this.date[1];\r\n                this.payoutReportData()\r\n            },\r\n            hanldReset () {\r\n                var myDate = new Date();\r\n                var year = myDate.getFullYear()\r\n                var month = (myDate.getMonth() + 1) < 10 ? \"0\" + (myDate.getMonth() + 1) : myDate.getMonth() + 1;\r\n                var day = myDate.getDate()\r\n\r\n                this.date = [year + \"-\" + month + \"-01\", year + \"-\" + month + \"-\" + day];\r\n                this.Start = year + \"-\" + month + \"-01\";\r\n                this.End = year + \"-\" + month + \"-\" + day;\r\n            },\r\n        },\r\n        mounted() {\r\n            this.payoutReportData()\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n</style>"],"sourceRoot":"src/views/reportCenter"}]}