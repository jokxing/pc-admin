AAA,WAAA;AAAA,QAAA,OAAA,EAAA;AAAA,OADA,CAbA,CAfA;AAgCA,MAAA,iBAAA,EAAA;AAhCA,KAAA;AAkCA,GApCA;AAqCA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,gBAFA,4BAEA,IAFA,EAEA,QAFA,EAEA;AACA,WAAA,cAAA,GAAA,QAAA,CAAA,MAAA,IAAA,CAAA;AACA,WAAA,SAAA,GAAA,QAAA,CAAA,MAAA;AACA,KALA;AAMA;AACA,IAAA,YAPA,wBAOA,IAPA,EAOA,QAPA,EAOA;AACA,WAAA,cAAA,GAAA,QAAA,CAAA,MAAA,IAAA,CAAA;AACA,WAAA,SAAA,GAAA,QAAA,CAAA,MAAA;AACA,KAVA;AAWA;AACA,IAAA,wBAZA,oCAYA,IAZA,EAYA;AACA,WAAA,cAAA,GAAA,IAAA,CAAA,GAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,KAfA;AAgBA;AACA,IAAA,kBAjBA,8BAiBA,IAjBA,EAiBA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,IAAA,KAAA,YAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,IAAA,KAAA,WAAA;AACA,UAAA,OAAA,GAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,IAAA,GAAA,EAAA;;AAEA,UAAA,CAAA,KAAA,IAAA,CAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,yBAAA;AACA;;AACA,UAAA,CAAA,OAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,kBAAA;AACA;;AACA,aAAA,KAAA,IAAA,KAAA,IAAA,OAAA;AACA,KA7BA;AA8BA;AACA,IAAA,UA/BA,sBA+BA,QA/BA,EA+BA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,SAAA,CAAA;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AACA,iBAAA,KAAA;AACA;AACA,OARA;AASA;AAzCA;AArCA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <h3 class=\"title\">会员调级申请</h3>\r\n\r\n        <el-row class=\"memberApply_form\">\r\n            <h4>上传图片（{{ uploadNum }}/5）<span>文件格式为png, jpg, jpeg 且大小不超过15MB</span></h4>\r\n            <el-upload\r\n                action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n                list-type=\"picture-card\"\r\n                :limit=\"5\"\r\n                :class=\"{hide:hideUploadEdit}\"\r\n                :on-change=\"handleEditChange\"\r\n                :on-preview=\"handlePictureCardPreview\"\r\n                :on-remove=\"handleRemove\"\r\n                :before-upload=\"beforeAvatarUpload\">\r\n                <i class=\"el-icon-plus\"></i>\r\n            </el-upload>\r\n            <el-dialog :visible.sync=\"dialogVisible\">\r\n                <img width=\"100%\" :src=\"dialogImageUrl\" alt=\"\">\r\n            </el-dialog>\r\n        </el-row>\r\n\r\n        <div class=\"memberApply_form\" v-loading.fullscreen.lock=\"fullscreenLoading\">\r\n            <h4>申请信息</h4>\r\n            <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"110px\">\r\n                <el-form-item label=\"会员帐号\" prop=\"name\">\r\n                    <el-input placeholder=\"请输入会员帐号\" v-model=\"ruleForm.name\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"其他平台等级\" prop=\"level\">\r\n                    <el-select v-model=\"ruleForm.level\" placeholder=\"请选择其他平台等级\">\r\n                        <el-option label=\"1\" value=\"1\"></el-option>\r\n                        <el-option label=\"2\" value=\"2\"></el-option>\r\n                        <el-option label=\"3\" value=\"3\"></el-option>\r\n                        <el-option label=\"4\" value=\"4\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"其他平台链接\" prop=\"url\">\r\n                    <el-input placeholder=\"请输入其他平台链接\" v-model=\"ruleForm.url\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"其他平台帐号\" prop=\"otherName\">\r\n                    <el-input placeholder=\"请输入其他平台帐号\" v-model=\"ruleForm.otherName\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"其他平台密码\" prop=\"otherPass\">\r\n                    <el-input placeholder=\"请输入其他平台密码\" v-model=\"ruleForm.otherPass\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"备注\" prop=\"remarks\">\r\n                    <el-input type=\"textarea\" v-model=\"ruleForm.remarks\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button class=\"primary-button\" @click=\"submitForm('ruleForm')\">提交申请</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        data() {\r\n            return {\r\n                // 上传\r\n                dialogImageUrl: '',\r\n                dialogVisible: false,\r\n                hideUploadEdit: false,\r\n                uploadNum: 0,\r\n                // 申请信息\r\n                ruleForm:{\r\n                    name: \"\",\r\n                    level: \"\",\r\n                    url: \"\",\r\n                    otherName: \"\",\r\n                    otherPass: \"\",\r\n                    remarks: \"\"\r\n                },\r\n                rules: {\r\n                    name: [\r\n                        { required: true, message: '请输入会员帐号', trigger: 'blur' },\r\n                    ],\r\n                    level: [\r\n                        { required: true, message: '请选择其他平台等级', trigger: 'change' },\r\n                    ],\r\n                    url: [\r\n                        { required: true, message: '请输入其他平台链接', trigger: 'blur' },\r\n                    ],\r\n                    otherName: [\r\n                        { required: true, message: '请输入其他平台帐号', trigger: 'blur' },\r\n                    ],\r\n                    url: [\r\n                        { required: true, message: '请输入其他平台密码', trigger: 'blur' },\r\n                    ],\r\n                },\r\n                fullscreenLoading: false\r\n            }\r\n        },\r\n        methods: {\r\n            // 上传change事件\r\n            handleEditChange(file, fileList) {\r\n                this.hideUploadEdit = fileList.length >= 5;\r\n                this.uploadNum = fileList.length\r\n            },\r\n            // 上传删除事件\r\n            handleRemove(file, fileList) {\r\n                this.hideUploadEdit = fileList.length >= 5;\r\n                this.uploadNum = fileList.length\r\n            },\r\n            // 放大上传的图片\r\n            handlePictureCardPreview(file) {\r\n                this.dialogImageUrl = file.url;\r\n                this.dialogVisible = true;\r\n            },\r\n            // 上传图片限制格式和大小\r\n            beforeAvatarUpload(file) {\r\n                const isJPG = file.type === 'image/jpeg';\r\n                const isPNG = file.type === 'image/png'\r\n                const isLt15M = file.size / 1024 / 1024 < 15;\r\n\r\n                if (!isJPG && !isPNG) {\r\n                    this.$message.error('上传头像图片只能是 JPG 和 PNG 格式!');\r\n                }\r\n                if (!isLt15M) {\r\n                    this.$message.error('上传图片大小不能超过 15MB!');\r\n                }\r\n                return isJPG || isPNG && isLt15M;\r\n            },\r\n            // 申请提交\r\n            submitForm(formName) {\r\n                this.$refs[formName].validate((valid) => {\r\n                    if (valid) {\r\n                        alert('submit!');\r\n                        this.fullscreenLoading = true;\r\n                    } else {\r\n                        console.log('error submit!!');\r\n                        return false;\r\n                    }\r\n                });\r\n            },\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/management.css);\r\n</style>"],"sourceRoot":"src/views/management"}]}