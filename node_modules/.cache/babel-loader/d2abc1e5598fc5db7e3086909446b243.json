AACA,WAAA,IAAA,CAAA,GAAA,GAAA,KAAA,IAAA,CAAA,CAAA,CAAA;;AAEA,UAAA,IAAA,GAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,eAAA;AACA,OAFA,MAEA;AACA,aAAA,YAAA;AACA;AACA,KArDA;AAsDA;AACA,IAAA,KAvDA,mBAuDA;AACA,UAAA,MAAA,GAAA,IAAA,IAAA,EAAA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,MAAA,CAAA,WAAA,KAAA,GAAA,IAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,KAAA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,MAAA,CAAA,WAAA,KAAA,GAAA,IAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,MAAA,CAAA,OAAA,EAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,CAAA;AAEA,WAAA,IAAA,GAAA,CAAA,MAAA,CAAA,WAAA,KAAA,GAAA,IAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,KAAA,EAAA,MAAA,CAAA,WAAA,KAAA,GAAA,IAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,MAAA,CAAA,OAAA,EAAA,CAAA;AAEA,WAAA,WAAA,GAAA,IAAA;AACA,KAlEA;AAmEA;AACA,IAAA,mBApEA,+BAoEA,GApEA,EAoEA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,YAAA;AACA;AAvEA,GA5BA;AAqGA,EAAA,OArGA,qBAqGA;AACA,SAAA,YAAA;AACA;AAvGA,CAAA","sourcesContent":["<template>\r\n    <div class=\"gameRecord\">\r\n        <h3 class=\"title\">存提款记录</h3>\r\n\r\n        <el-row :gutter=\"6\" class=\"head\">\r\n            <el-form>\r\n                <el-form-item style=\"margin-bottom: 10px\">\r\n                    <el-col :span=\"7\">\r\n                        <el-input placeholder=\"会员账号\" v-model=\"data.Name\"></el-input>\r\n                    </el-col>\r\n                    <el-col :span=\"5\">\r\n                        <el-select placeholder=\"请选择资金类型\" v-model=\"data.Act\">\r\n                            <el-option label=\"存款\" value=\"1\"></el-option>\r\n                            <el-option label=\"提款\" value=\"2\"></el-option>\r\n                            <el-option label=\"代理存款\" value=\"3\"></el-option>\r\n                            <el-option label=\"代理彩金\" value=\"4\"></el-option>\r\n                        </el-select>\r\n                    </el-col>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-col :span=\"7\">\r\n                        <el-date-picker\r\n                            v-model=\"date\"\r\n                            type=\"daterange\"\r\n                            :clearable=\"false\"\r\n                            :picker-options=\"pickerOptions\"\r\n                            range-separator=\"至\"\r\n                            value-format=\"yyyy-MM-dd\"\r\n                            start-placeholder=\"开始日期\"\r\n                            end-placeholder=\"结束日期\">\r\n                        </el-date-picker>\r\n                    </el-col>\r\n                    <el-col :span=\"8\">\r\n                         <el-radio-group v-model=\"tabPosition\" @change=\"tabChange\">\r\n                            <el-radio-button label=\"1\">今日</el-radio-button>\r\n                            <el-radio-button label=\"2\">昨日</el-radio-button>\r\n                            <el-radio-button label=\"3\">7天</el-radio-button>\r\n                            <el-radio-button label=\"4\">30天</el-radio-button>\r\n                        </el-radio-group>\r\n                    </el-col>\r\n                    <el-col :span=\"8\">\r\n                        <el-button round class=\"primary-button\" @click=\"query()\">查询</el-button>\r\n                        <el-button round plain @click=\"reset()\">重置</el-button>\r\n                    </el-col>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-row>\r\n\r\n        <!-- 表格 -->\r\n        <el-row>\r\n            <el-table\r\n                :data=\"tableData\"\r\n                style=\"width: 100%\"\r\n                v-loading=\"tableLoading\">\r\n                <el-table-column\r\n                    prop=\"id\"\r\n                    label=\"序号\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"name\"\r\n                    label=\"会员账号\"\r\n                    width=\"170\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"realname\"\r\n                    label=\"昵称\"\r\n                    width=\"170\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"money\"\r\n                    label=\"金额\"\r\n                    width=\"150\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"actname\"\r\n                    label=\"资金类型\"\r\n                    width=\"150\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"created_at\"\r\n                    label=\"完成时间\"\r\n                    width=\"160\">\r\n                </el-table-column>\r\n            </el-table>\r\n        </el-row>\r\n\r\n        <!-- 分页 -->\r\n        <el-pagination\r\n            background\r\n            layout=\"prev, pager, next\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page.sync=\"data.Page\" \r\n            :total=\"total\"\r\n            :page-size=\"pagesize\"\r\n            :hide-on-single-page=\"true\">\r\n        </el-pagination>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import { UserFundApi } from '../../http/api.js'\r\n    export default {\r\n        data() {\r\n            var myDate = new Date();\r\n            return {\r\n                tableLoading: true,\r\n                // 查询\r\n                date: [myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\", myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate()],\r\n                pickerOptions: {\r\n                    disabledDate: (time) => {\r\n                        return time.getTime() < (myDate.getTime() - 24*60*60*1000*31) || time.getTime() >= (Date.now())\r\n                    }\r\n                },\r\n                data: {\r\n                    Act: \"\",\r\n                    Start: myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\",\r\n                    End: myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate(),\r\n                    Name: \"\",\r\n                    Page: 1\r\n                },\r\n                game:[],\r\n                tabPosition: '-1',\r\n                // 表格\r\n                tableData:[],\r\n                // 分页\r\n                total: 0,\r\n                pagesize: 0\r\n            }\r\n        },   \r\n        methods: {\r\n            // 初始化\r\n            userFundData () {\r\n                var _this = this;\r\n                this.tableLoading = true;\r\n                UserFundApi(_this.data).then(res => {\r\n                    _this.tableLoading = false;\r\n                    _this.tableData = res.data.message.list;\r\n                    _this.total = res.data.message.total;\r\n                    _this.pagesize = res.data.message.Pagesize\r\n                }).catch(err => {\r\n                    _this.tableLoading = false\r\n                })\r\n            },\r\n            // 选择时间\r\n            tabChange: function (tab, event) {\r\n                var myDate = new Date();\r\n                if(tab == 1){\r\n                    this.date = [this.getDate(myDate.getTime()), this.getDate(myDate.getTime())]\r\n                }else if(tab == 2){\r\n                    var yesterday = myDate.getTime() - 24*60*60*1000;\r\n                    this.date = [this.getDate(yesterday), this.getDate(myDate.getTime())]\r\n                }else if(tab == 3){\r\n                    var seven = myDate.getTime() - 24*60*60*1000*7;\r\n                    this.date = [this.getDate(seven), this.getDate(myDate.getTime())]\r\n                }else{\r\n                    var thirty = myDate.getTime() - 24*60*60*1000*30;\r\n                    this.date = [this.getDate(thirty), this.getDate(myDate.getTime())]\r\n                }\r\n            },\r\n            // 计算时间戳\r\n            getDate: function (time) {\r\n                var myDate = new Date(time);\r\n                var year = myDate.getFullYear();\r\n                var month = myDate.getMonth() + 1;\r\n                var day = myDate.getDate();\r\n                return year + \"-\" + month + \"-\" + day;\r\n            },\r\n            // 查询\r\n            query: function () {\r\n                var date1 = new Date(this.date[0]).getTime();\r\n                var date2 = new Date(this.date[1]).getTime();\r\n                var dates = date2 - date1;\r\n                var days = dates / (24*60*60*1000)\r\n\r\n                this.data.Start = this.date[0];\r\n                this.data.End = this.date[1]\r\n\r\n                if(days > 30){\r\n                    this.$message.error('只能查询最近30天的时间。');\r\n                }else{\r\n                    this.userFundData()\r\n                }\r\n            },\r\n            // 重置\r\n            reset () {\r\n                var myDate = new Date();\r\n                this.data.Act = \"\";\r\n                this.data.Start = myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\";\r\n                this.data.End = myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate();\r\n                this.data.Name = \"\";\r\n                this.data.Page = 1;\r\n\r\n                this.date = [myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\", myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate()];\r\n\r\n                this.tabPosition = \"-1\"\r\n            },\r\n            // 分页\r\n            handleCurrentChange(val) {\r\n                this.data.Page = val;\r\n                this.userFundData()\r\n            }\r\n        },\r\n        mounted() {\r\n            this.userFundData()\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/memberRecord.css);\r\n</style>"],"sourceRoot":"src/views/memberRecord"}]}