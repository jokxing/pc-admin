=\"content\"\r\n                    label=\"投注内容\"\r\n                    width=\"100\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"payout\"\r\n                    label=\"派彩\"\r\n                    sortable\r\n                    width=\"80\">\r\n                        <template slot-scope=\"scope\">\r\n                            <span :class=\"{ 'red': scope.row.payout < 0, 'green': scope.row.payout > 0 }\">\r\n                                {{ scope.row.payout }}\r\n                            </span>\r\n                        </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"beat_money\"\r\n                    label=\"投注金额\"\r\n                    sortable\r\n                    width=\"108\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                    prop=\"valid_betamt\"\r\n                    label=\"有效投注\"\r\n                    sortable\r\n                    width=\"108\">\r\n                </el-table-column>\r\n            </el-table>\r\n        </el-row>\r\n\r\n        <!-- 分页 -->\r\n        <el-pagination\r\n            background\r\n            layout=\"prev, pager, next\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page.sync=\"page\" \r\n            :total=\"total\"\r\n            :page-size=\"pagesize\"\r\n            :hide-on-single-page=\"true\">\r\n        </el-pagination>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import { GameReportDetailApi } from '../../http/api.js'\r\n    export default {\r\n        data() {\r\n            return {\r\n                tableLoading: true,\r\n                tableData: [],\r\n                page: 1,\r\n                pagesize: 0,\r\n                total: 0,\r\n                Sort: \"\",\r\n                SortType: \"\",\r\n                data: {}\r\n            }\r\n        },   \r\n        methods: {\r\n            // 初始化\r\n            GameReportDetailData() {\r\n                var _this = this;\r\n                this.tableLoading = true;\r\n                _this.data = this.$cookies.get('gameArr');\r\n                _this.data[\"Page\"] = this.page;\r\n                _this.data[\"Sort\"] = this.Sort;\r\n                _this.data['SortType'] = this.SortType\r\n\r\n                GameReportDetailApi(_this.data).then(res => {\r\n                    _this.tableLoading = false;\r\n                    _this.tableData = res.data.message.lists;\r\n                    _this.total = res.data.message.total;\r\n                    _this.pagesize = res.data.message.PageSize\r\n                }).catch(err => {\r\n                    _this.tableLoading = false;\r\n                })\r\n            },\r\n            // 排序\r\n            sortChange (column, prop, order) {\r\n                this.Sort = column.prop;\r\n                if(column.order == \"ascending\"){\r\n                    this.SortType = \"Asc\"\r\n                }else{\r\n                    this.SortType = \"Desc\"\r\n                }\r\n                this.GameReportDetailData()\r\n            },\r\n            // 返回\r\n            back () {\r\n                var Name = this.$cookies.get('gameArr').Name;\r\n                this.$router.push({\r\n                    name: 'gameRecord',\r\n                    params: {\r\n                        name: Name\r\n                    }\r\n                });\r\n                this.$cookies.remove('gameArr')\r\n            },\r\n            // 分页\r\n            handleCurrentChange(val) {\r\n                this.page = val;\r\n                this.GameReportDetailData()\r\n            }\r\n        },\r\n        mounted() {\r\n            this.GameReportDetailData()\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/memberRecord.css);\r\n</style>"],"sourceRoot":"src/views/memberRecord"}]}