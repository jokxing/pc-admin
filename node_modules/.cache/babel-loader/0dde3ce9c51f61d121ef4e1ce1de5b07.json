AAA,KAAA,EAAA,KAAA,KADA;AAEA,QAAA,GAAA,EAAA,KAAA,GAFA;AAGA,QAAA,IAAA,EAAA,KAAA,IAHA;AAIA,QAAA,IAAA,EAAA,KAAA,IAJA;AAKA,QAAA,IAAA,EAAA,KAAA,IALA;AAMA,QAAA,QAAA,EAAA,KAAA;AANA,OAAA;AAQA,WAAA,YAAA,GAAA,IAAA;AACA,MAAA,aAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA,QAAA;AACA,OALA,EAKA,KALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,KAAA;AACA,OAPA;AAQA,KArBA;AAsBA;AACA,IAAA,UAAA,EAAA,sBAAA;AACA,WAAA,IAAA,GAAA,CAAA;AACA,WAAA,KAAA,GAAA,KAAA,IAAA,CAAA,CAAA,CAAA;AACA,WAAA,GAAA,GAAA,KAAA,IAAA,CAAA,CAAA,CAAA;AACA,WAAA,cAAA;AACA,KA5BA;AA6BA;AACA,IAAA,UAAA,EAAA,sBAAA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,GAAA,IAAA,IAAA,EAAA;AACA,WAAA,IAAA,GAAA,CAAA,MAAA,CAAA,WAAA,KAAA,GAAA,IAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,KAAA,EAAA,MAAA,CAAA,WAAA,KAAA,GAAA,IAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,MAAA,CAAA,OAAA,EAAA,CAAA;AACA,WAAA,KAAA,GAAA,MAAA,CAAA,WAAA,KAAA,GAAA,IAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,KAAA;AACA,WAAA,GAAA,GAAA,MAAA,CAAA,WAAA,KAAA,GAAA,IAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,MAAA,CAAA,OAAA,EAAA;AACA,KApCA;AAqCA;AACA,IAAA,UAtCA,sBAsCA,GAtCA,EAsCA;AACA;AACA,UAAA,GAAA,CAAA,IAAA,IAAA,WAAA,EAAA;AACA,aAAA,IAAA,GAAA,KAAA;AACA,OAFA,MAEA,IAAA,GAAA,CAAA,IAAA,IAAA,gBAAA,EAAA;AACA,aAAA,IAAA,GAAA,KAAA;AACA,OAFA,MAEA,IAAA,GAAA,CAAA,IAAA,IAAA,SAAA,EAAA;AACA,aAAA,IAAA,GAAA,YAAA;AACA,OAFA,MAEA;AACA,aAAA,IAAA,GAAA,UAAA;AACA,OAVA,CAWA;;;AACA,UAAA,GAAA,CAAA,KAAA,IAAA,YAAA,EAAA;AACA,aAAA,QAAA,GAAA,MAAA;AACA,OAFA,MAEA;AACA,aAAA,QAAA,GAAA,KAAA;AACA,OAhBA,CAiBA;;;AACA,WAAA,IAAA,GAAA,CAAA;AAEA,WAAA,cAAA;AACA,KA3DA;AA4DA;AACA,IAAA,mBA7DA,+BA6DA,GA7DA,EA6DA;AACA,WAAA,IAAA,GAAA,GAAA;AACA,WAAA,cAAA;AACA;AAhEA,GAvBA;AAyFA,EAAA,OAzFA,qBAyFA;AACA,SAAA,cAAA;AACA;AA3FA,CAAA","sourcesContent":["<template>\r\n    <div class=\"memberManage\">\r\n        <h4 class=\"title\">会员管理</h4>\r\n        <el-row :gutter=\"20\">\r\n            <el-form>\r\n                <el-form-item>\r\n                    <el-col :span=\"7\">\r\n                        <el-date-picker\r\n                            v-model=\"date\"\r\n                            type=\"daterange\"\r\n                            :clearable=\"false\"\r\n                            range-separator=\"至\"\r\n                            start-placeholder=\"开始日期\"\r\n                            end-placeholder=\"结束日期\"\r\n                            value-format=\"yyyy-MM-dd\"\r\n                            :picker-options=\"pickerOptions\">\r\n                        </el-date-picker>\r\n                    </el-col>\r\n                    <el-col :span=\"5\">\r\n                        <el-input v-model=\"Name\" placeholder=\"会员账号\"></el-input>\r\n                    </el-col>\r\n                    <el-col :span=\"6\" class=\"form_button\">\r\n                        <el-button class=\"primary-button\" round @click=\"hanldQuery\">查询</el-button>\r\n                        <el-button plain round @click=\"hanldReset\">重置</el-button>\r\n                    </el-col>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-row>\r\n\r\n        <!-- 表格 -->\r\n        <el-row>\r\n            <el-table\r\n                :data=\"tableData\"\r\n                style=\"width: 100%\"\r\n                v-loading=\"tableLoading\"\r\n                @sort-change=\"changeSort\">\r\n                    <el-table-column\r\n                        prop=\"id\"\r\n                        label=\"ID\"\r\n                        width=\"70\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"name\"\r\n                        label=\"帐号\"\r\n                        width=\"110\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"account\"\r\n                        label=\"姓名\"\r\n                        width=\"65\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"level_name\"\r\n                        label=\"等级\"\r\n                        width=\"70\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"user_dpt_money\"\r\n                        label=\"存款\"\r\n                        sortable='custom'\r\n                        width=\"80\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"wtd_money\"\r\n                        label=\"取款\"\r\n                        sortable='custom'\r\n                        width=\"80\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"ins_win\"\r\n                        label=\"公司总输赢\"\r\n                        width=\"100\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        label=\"状态\"\r\n                        width=\"90\">\r\n                            <template slot-scope=\"scope\" >\r\n                                <span v-if=\"scope.row.status_name == '静止户'\">{{ scope.row.status_name }}</span>\r\n                                <span class=\"red\" v-if=\"scope.row.status_name == '停权户'\">{{ scope.row.status_name }}</span>\r\n                                <span class=\"yellow\" v-if=\"scope.row.status_name == '试玩'\">{{ scope.row.status_name }}</span>\r\n                                <span class=\"red\" v-if=\"scope.row.status_name == '密码锁定'\">{{ scope.row.status_name }}</span>\r\n                                <span class=\"grey\" v-if=\"scope.row.status_name == '注销'\">{{ scope.row.status_name }}</span>\r\n                                <span class=\"green\" v-if=\"scope.row.status_name == '正常户'\">{{ scope.row.status_name }}</span>\r\n                            </template>\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"addTime\"\r\n                        label=\"注册时间/ip\"\r\n                        sortable='custom'\r\n                        width=\"150\">\r\n                            <template slot-scope=\"scope\">\r\n                                {{ scope.row.addTime }} / {{ scope.row.addIp }}\r\n                            </template>\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"last_login_time\"\r\n                        label=\"登陆时间/ip\"\r\n                        sortable='custom'\r\n                        width=\"150\">\r\n                            <template slot-scope=\"scope\">\r\n                                {{ scope.row.last_login_time }} / {{ scope.row.last_login_ip }}\r\n                            </template>\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"agent_remark\"\r\n                        label=\"备注\"\r\n                        width=\"100\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        prop=\"agent_wechat_qq\"\r\n                        label=\"QQ/微信\"\r\n                        width=\"90\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                        label=\"操作\"\r\n                        width=\"170\">\r\n                        <el-button type=\"text\">会员代存</el-button>\r\n                        <el-button type=\"text\">彩金赠送</el-button>\r\n                    </el-table-column>\r\n            </el-table>\r\n        </el-row>\r\n\r\n        <!-- 分页 -->\r\n        <el-pagination\r\n            background\r\n            layout=\"prev, pager, next\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page.sync=\"page\" \r\n            :total=\"total\"\r\n            :page-size=\"pagesize\"\r\n            :hide-on-single-page=\"true\">\r\n        </el-pagination>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import { MemberListApi } from '../../http/api.js'\r\n    export default {\r\n        data() {\r\n            var myDate = new Date();\r\n            return {\r\n                tableLoading: true,\r\n                date: [myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\", myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate()],\r\n                pickerOptions: {\r\n                    disabledDate(time) {\r\n                        return time.getTime() >= (Date.now());\r\n                    }\r\n                },\r\n                Start: myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\",\r\n                End: myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate(),\r\n                Name: \"\",\r\n                Sort: \"\",\r\n                SortType: \"\",\r\n                //  表格\r\n                tableData: [],\r\n                page: 1,\r\n                total: 0,\r\n                pagesize: 0\r\n            }\r\n        },   \r\n        methods: {\r\n            // 初始化\r\n            MemberListData () {\r\n                var _this = this;\r\n                var data = {\r\n                    Start: this.Start,\r\n                    End: this.End,\r\n                    Name: this.Name,\r\n                    Page: this.page,\r\n                    Sort: this.Sort,\r\n                    SortType: this.SortType\r\n                };\r\n                this.tableLoading = true;\r\n                MemberListApi(data).then(res => {\r\n                    _this.tableLoading = false;\r\n                    _this.tableData = res.data.message.Data;\r\n                    _this.total = res.data.message.Total;\r\n                    _this.pagesize = res.data.message.Pagesize\r\n                }).catch(err => {\r\n                    _this.tableLoading = false\r\n                })\r\n            },\r\n            // 查询\r\n            hanldQuery: function () {\r\n                this.page = 1;\r\n                this.Start = this.date[0];\r\n                this.End = this.date[1];\r\n                this.MemberListData()\r\n            },\r\n            // 重置\r\n            hanldReset: function () {\r\n                this.Name = \"\";\r\n                var myDate = new Date();\r\n                this.date = [myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\", myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate()];\r\n                this.Start = myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\";\r\n                this.End = myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate();\r\n            },\r\n            // 排序\r\n            changeSort (val) {\r\n                // 获取排序的参数\r\n                if(val.prop == \"wtd_money\"){\r\n                    this.Sort = 'wtd'\r\n                }else if(val.prop == \"user_dpt_money\"){\r\n                    this.Sort = 'dpt'\r\n                }else if (val.prop == 'addTime'){\r\n                    this.Sort = 'created_at'\r\n                }else{\r\n                    this.Sort = 'login_at'\r\n                }\r\n                // 顺序还是倒序\r\n                if(val.order == \"descending\"){\r\n                    this.SortType = 'Desc'\r\n                }else{\r\n                    this.SortType = 'Asc'\r\n                }\r\n                // 默认第一页\r\n                this.page = 1;\r\n\r\n                this.MemberListData()\r\n            },\r\n            // 分页\r\n            handleCurrentChange(val) {\r\n                this.page = val;\r\n                this.MemberListData()\r\n            }\r\n        },\r\n        mounted() {\r\n            this.MemberListData()\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/management.css);\r\n</style>"],"sourceRoot":"src/views/management"}]}