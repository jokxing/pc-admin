ion>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"状态\" prop=\"state\">\r\n                    <el-select v-model=\"ruleForm.state\" placeholder=\"请选择状态\">\r\n                        <el-option label=\"1\" value=\"1\"></el-option>\r\n                        <el-option label=\"2\" value=\"2\"></el-option>\r\n                        <el-option label=\"3\" value=\"3\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"原因\" prop=\"reason\">\r\n                    <el-input type=\"textarea\" v-model=\"ruleForm.reason\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button class=\"primary-button\" @click=\"submitForm('ruleForm')\">提交申请</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        data() {\r\n            return {\r\n                ruleForm:{\r\n                    date: \"\",\r\n                    name: \"\",\r\n                    oldLevel: \"\",\r\n                    newLevel: \"\",\r\n                    state: \"\",\r\n                    reason: \"\"\r\n                },\r\n                rules: {\r\n                    date: [\r\n                        { required: true, message: '请选择日期', trigger: 'blur' },\r\n                    ],\r\n                    name: [\r\n                        { required: true, message: '请输入会员用户名', trigger: 'blur' },\r\n                    ],\r\n                    oldLevel: [\r\n                        { required: true, message: '请选择原等级', trigger: 'change' },\r\n                    ],\r\n                    newLevel: [\r\n                        { required: true, message: '请选择等级', trigger: 'change' },\r\n                    ],\r\n                    state: [\r\n                        { required: true, message: '请选择状态', trigger: 'change' },\r\n                    ],\r\n                },\r\n                fullscreenLoading: false\r\n            }\r\n        },\r\n        methods: {\r\n            // 申请提交\r\n            submitForm(formName) {\r\n                this.$refs[formName].validate((valid) => {\r\n                    if (valid) {\r\n                        alert('submit!');\r\n                        this.fullscreenLoading = true;\r\n                    } else {\r\n                        console.log('error submit!!');\r\n                        return false;\r\n                    }\r\n                });\r\n            },\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/management.css);\r\n</style>"],"sourceRoot":"src/views/management"}]}