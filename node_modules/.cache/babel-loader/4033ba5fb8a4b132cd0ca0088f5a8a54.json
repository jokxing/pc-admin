r\n        <h4 class=\"title\">公告</h4>\r\n        <el-row class=\"notice_tab\">\r\n            <el-col :span=\"6\" v-for=\"(item, index) in tabs\" :key=\"index\" :class=\"{ 'tab_active' : num == index }\">\r\n                <div @click=\"noticeTabClick(item, index)\">{{ item }}</div>\r\n            </el-col>\r\n        </el-row>\r\n\r\n        <el-table\r\n            :data=\"tableData\"\r\n            style=\"width: 100%;cursor: pointer\"\r\n            :show-header=\"false\"\r\n            class=\"notice_table\"\r\n            id=\"notice_table\"\r\n            @row-click=\"openDetails\">\r\n            <el-table-column\r\n                width=\"40\">\r\n                <i class=\"iconfont icon-notice primary-color\"></i>\r\n            </el-table-column>\r\n            <el-table-column\r\n                width=\"120\">\r\n                {{ type }}\r\n            </el-table-column>\r\n            <el-table-column\r\n                prop=\"title\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                prop=\"created_at\"\r\n                width=\"180\"\r\n                style=\"text-align: center;\">\r\n            </el-table-column>\r\n        </el-table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import { noticeApi } from '../../http/api.js'\r\n    export default {\r\n        data() {\r\n            return {\r\n                noticeLoading: true,\r\n                tabs:['会员讯息','体育公告','真人公告','彩票公告'],\r\n                num: 0,\r\n                type: \"会员讯息\",\r\n                tableData: [],\r\n            }\r\n        },\r\n        methods: {\r\n            // 初始化\r\n            noticeInit () {\r\n                var _this = this;\r\n                var data = {\r\n                    type: this.type\r\n                }\r\n                this.noticeLoading = true;\r\n                noticeApi(data).then(res => {\r\n                    _this.noticeLoading = false;\r\n                    _this.tableData = res.data.message\r\n                }).catch(err => {\r\n                    _this.noticeLoading = false;\r\n                })\r\n            },\r\n            // tab事件\r\n            noticeTabClick (item, index) {\r\n                this.num = index;\r\n                this.type = item;\r\n                this.noticeInit()\r\n            },\r\n            // 点击表格事件\r\n            openDetails (row, column, event) {\r\n                this.$router.push('/notice/noticeDetails/' + row.id)\r\n            },\r\n        },\r\n        mounted() {\r\n            this.noticeInit()\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .notice_tab{\r\n        border-bottom: 1px #eee solid;\r\n        margin-bottom: 30px;\r\n    }\r\n    .notice_tab .el-col{\r\n        text-align: center;\r\n        line-height: 36px;\r\n        cursor: pointer;\r\n        border-radius: 5px 5px 0 0;\r\n    }\r\n    .notice i{\r\n        font-size: 20px;\r\n    }\r\n\r\n    .notice .el-table::before{\r\n        height: 0;\r\n    }\r\n</style>"],"sourceRoot":"src/views/notice"}]}