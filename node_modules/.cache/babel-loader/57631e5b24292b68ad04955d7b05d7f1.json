,UAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;AACA,UAAA,aAAA,CAAA,KAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,iBAAA,GAAA,KAAA,CADA,CAEA;;AACA,YAAA,KAAA,CAAA,KAAA,CAAA,QAAA,EAAA,WAAA,GAHA,CAIA;;;AACA,YAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA;;AACA,YAAA,KAAA,CAAA,MAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,IAAA,GAAA,EAAA,CAPA,CAQA;;AACA,YAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,WAVA,EAUA,KAVA,CAUA,UAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,YAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAbA;AAcA,SAnBA,MAmBA;AACA,iBAAA,KAAA;AACA;AACA,OAvBA;AAwBA;AAnFA,GApCA;AAyHA,EAAA,OAzHA,qBAyHA;AACA,SAAA,SAAA;AACA,GA3HA;AA4HA,EAAA,UAAA,EAAA;AACA,IAAA,UAAA,EAAA;AADA;AA5HA,CAAA","sourcesContent":["<template>\r\n    <div class=\"memberLineApply\" v-loading.fullscreen.lock=\"fullscreenLoading\">\r\n        <h3 class=\"title\">会员转线申请</h3>\r\n\r\n        <statistics></statistics>\r\n\r\n        <el-row class=\"memberApply_form\">\r\n            <h4>上传图片（{{ uploadNum }}/5）<span>文件格式为png, jpg, jpeg 且大小不超过15MB</span></h4>\r\n            <el-upload\r\n                :action=\"upUrl\"\r\n                :data=\"qn\"\r\n                list-type=\"picture-card\"\r\n                ref=\"upload\"\r\n                :limit=\"5\"\r\n                :class=\"{hide:hideUploadEdit}\"\r\n                :on-change=\"handleEditChange\"\r\n                :on-preview=\"handlePictureCardPreview\"\r\n                :on-remove=\"handleRemove\"\r\n                :before-upload=\"beforeAvatarUpload\"\r\n                :on-success=\"handleSuccess\">\r\n                <i class=\"el-icon-plus\"></i>\r\n            </el-upload>\r\n            <el-dialog :visible.sync=\"dialogVisible\">\r\n                <img width=\"100%\" :src=\"dialogImageUrl\" alt=\"\">\r\n            </el-dialog>\r\n        </el-row>\r\n\r\n        <el-row class=\"memberApply_form\">\r\n            <h4>申请信息</h4>\r\n            <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"80px\">\r\n                <el-form-item label=\"会员账号\" prop=\"member_name\">\r\n                    <el-input v-model=\"ruleForm.member_name\" maxlength=\"10\" placeholder=\"请输入会员账号\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"引导链接\" prop=\"guide_link\">\r\n                    <el-input v-model=\"ruleForm.guide_link\" placeholder=\"请输入引导链接\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"终端类型\" prop=\"terminal_type\">\r\n                    <el-select v-model=\"ruleForm.terminal_type\" placeholder=\"请选择终端\">\r\n                        <el-option label=\"苹果\" value=\"1\"></el-option>\r\n                        <el-option label=\"安卓\" value=\"2\"></el-option>\r\n                        <el-option label=\"PC电脑\" value=\"3\"></el-option>\r\n                        <el-option label=\"H5手机\" value=\"4\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"备注\" prop=\"remark\">\r\n                    <el-input type=\"textarea\" v-model=\"ruleForm.remark\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button class=\"primary-button\" @click=\"submitForm('ruleForm')\">提交申请</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-row>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import statistics from './statistics.vue'\r\n    import { ChangeLineApi, UpImgApi } from '../../http/api.js'\r\n    export default {\r\n        data() {\r\n            return {\r\n                dialogImageUrl: '',\r\n                dialogVisible: false,\r\n                hideUploadEdit: false,\r\n                uploadNum: 0,\r\n                qn:{\r\n                    token: \"\"\r\n                },\r\n                upUrl: \"\",\r\n                // 申请信息\r\n                ruleForm: {\r\n                    member_name: '',\r\n                    guide_link: \"\",\r\n                    terminal_type: \"\",\r\n                    remark: ''\r\n                },\r\n                rules: {\r\n                    member_name: [\r\n                        { required: true, message: '请输入会员账号', trigger: 'blur' },\r\n                    ],\r\n                    guide_link: [\r\n                        { required: true, message: '请输入引导链接', trigger: 'blur' },\r\n                    ],\r\n                    terminal_type: [\r\n                        { required: true, message: '请选择终端', trigger: 'change' },\r\n                    ],\r\n                },\r\n                imgs: \"\",\r\n                // 上传图片链接\r\n                Images: \"\",\r\n                ImagesNum: 0,\r\n                fullscreenLoading: false\r\n            }\r\n        },   \r\n        methods: {\r\n            // 上传初始化获取上传接口地址和token\r\n            UpImgData () {\r\n                var _this = this;\r\n                UpImgApi().then(res => {\r\n                    _this.upUrl = res.data.message.upUrl;\r\n                    _this.qn.token = res.data.message.Token\r\n                }).catch(err => {\r\n\r\n                })\r\n            },\r\n            // 上传change事件\r\n            handleEditChange(file, fileList) {\r\n                this.hideUploadEdit = fileList.length >= 5;\r\n                this.uploadNum = fileList.length\r\n            },\r\n            // 上传删除事件\r\n            handleRemove(file, fileList) {\r\n                this.imgs = '';\r\n                for(var i = 0; i < fileList.length; i++){\r\n                    this.imgs += fileList[i].response.file_path + \",\"\r\n                }\r\n                var lastIndex = this.imgs.lastIndexOf(',');\r\n                if (lastIndex > -1) {\r\n                    this.Images = this.imgs.substring(0, lastIndex) + this.imgs.substring(lastIndex + 1, this.imgs.length);\r\n                }\r\n                this.hideUploadEdit = fileList.length >= 5;\r\n                this.uploadNum = fileList.length\r\n            },\r\n            // 放大上传的图片\r\n            handlePictureCardPreview(file) {\r\n                this.dialogImageUrl = file.url;\r\n                this.dialogVisible = true;\r\n            },\r\n            // 上传图片限制格式和大小\r\n            beforeAvatarUpload(file) {\r\n                const isJPG = file.type === 'image/jpeg';\r\n                const isPNG = file.type === 'image/png'\r\n                const isLt15M = file.size / 1024 / 1024 < 15;\r\n\r\n                if (!isJPG && !isPNG) {\r\n                    this.$message.error('上传头像图片只能是 JPG 和 PNG 格式!');\r\n                }\r\n                if (!isLt15M) {\r\n                    this.$message.error('上传图片大小不能超过 15MB!');\r\n                }\r\n                return isJPG || isPNG && isLt15M;\r\n            },\r\n            // 上传成功\r\n            handleSuccess(res, file) {\r\n                this.imgs += res.file_path + \",\"\r\n                var lastIndex = this.imgs.lastIndexOf(',');\r\n                if (lastIndex > -1) {\r\n                    this.Images = this.imgs.substring(0, lastIndex) + this.imgs.substring(lastIndex + 1, this.imgs.length);\r\n                }\r\n            },\r\n            // 申请提交\r\n            submitForm(formName) {\r\n                var _this = this;\r\n                this.$refs[formName].validate((valid) => {\r\n                    if (valid) {\r\n                        if(this.Images != ''){\r\n                            this.ruleForm['Images'] = this.Images\r\n                        }\r\n                        this.fullscreenLoading = true;\r\n                        ChangeLineApi(_this.ruleForm).then(res => {\r\n                            _this.fullscreenLoading = false;\r\n                            // 清空表单\r\n                            _this.$refs[formName].resetFields();\r\n                            // 清空已上传文件\r\n                            _this.$refs.upload.clearFiles();\r\n                            _this.Images = \"\";\r\n                            _this.imgs = \"\";\r\n                            // 提示\r\n                            _this.$message.success(res.data.message);\r\n                        }).catch(err => {\r\n                            _this.fullscreenLoading = false;\r\n                            _this.$message.error(err.message)\r\n                        })\r\n                    } else {\r\n                        return false;\r\n                    }\r\n                });\r\n            },\r\n        },\r\n        created() {\r\n            this.UpImgData();\r\n        },\r\n        components: {\r\n            statistics\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/management.css);\r\n</style>"],"sourceRoot":"src/views/management"}]}