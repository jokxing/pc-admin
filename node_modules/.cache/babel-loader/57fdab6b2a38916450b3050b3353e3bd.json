=> {\r\n    return Service({\r\n        url: '/authorizations',\r\n        method: 'post',\r\n        data\r\n    })\r\n};\r\n\r\n// 基本信息\r\nexport const mainApi = data => {\r\n    return Service({\r\n        url: '/main',\r\n        method: 'get'\r\n    })\r\n}\r\n\r\n// 首页-数据展示\r\nexport const indexInfoApi = data => {\r\n    return Service({\r\n        url: '/indexInfo',\r\n        method: \"get\"\r\n    })\r\n}\r\n\r\n// 首页-报表\r\nexport const indexReportApi = data => {\r\n    return Service({\r\n        url: '/indexReport?Month=' + data,\r\n        method: \"get\"\r\n    })\r\n}\r\n\r\n// 公告列表\r\nexport const noticeApi = data => {\r\n    return Service({\r\n        url: '/notice',\r\n        method: 'post',\r\n        data\r\n    })\r\n}\r\n\r\n// 公告详情\r\nexport const noticeDetailApi = data => {\r\n    return Service({\r\n        url: '/noticeDetail',\r\n        method: 'post',\r\n        data\r\n    })\r\n}\r\n\r\n// 用户列表\r\nexport const MemberListApi = data => {\r\n    return Service({\r\n        url: '/MemberList',\r\n        method: 'post',\r\n        data\r\n    })\r\n}\r\n\r\n// 调线统计\r\nexport const MonthChangeLineRecordApi = data => {\r\n    return Service({\r\n        url: \"/MonthChangeLineRecord\",\r\n        method: 'get'\r\n    })\r\n}\r\n\r\n// 调级统计\r\nexport const MonthChangeLeveRecordApi = data => {\r\n    return Service({\r\n        url: \"/MonthChangeLeveRecord\",\r\n        method: 'get'\r\n    })\r\n}\r\n\r\n// 会员调线申请\r\nexport const ChangeLineApi = data => {\r\n    return Service({\r\n        url: \"/ChangeLine\",\r\n        method: 'post',\r\n        data\r\n    })\r\n}\r\n\r\n// 会员调线记录\r\nexport const ChangeLineLogsListApi = data => {\r\n    return Service({\r\n        url: \"/ChangeLineLogsList\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 会员调级申请\r\nexport const ChangeLeveApi = data => {\r\n    return Service({\r\n        url: \"/ChangeLeve\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 会员调级记录\r\nexport const ChangeLevelLogsListApi = data => {\r\n    return Service({\r\n        url: \"/ChangeLevelLogsList\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 上传图片初始化\r\nexport const UpImgApi = data => {\r\n    return Service({\r\n        url: \"/UpImg\",\r\n        method: \"get\"\r\n    })\r\n}\r\n\r\n// 游戏列表查询\r\nexport const GameReportApi = data => {\r\n    return Service({\r\n        url: \"/GameReport\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 游戏列表详情\r\nexport const GameReportDetailApi = data => {\r\n    return Service({\r\n        url: \"/GameReportDetail\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 存提款记录\r\nexport const UserFundApi = data => {\r\n    return Service({\r\n        url: \"/UserFund\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 提款金额\r\nexport const DrawMoneyInfoApi = data => {\r\n    return Service({\r\n        url: \"/DrawMoneyInfo\",\r\n        method: \"get\"\r\n    })\r\n}\r\n\r\n// 提款记录\r\nexport const DrawHisApi = data => {\r\n    return Service({\r\n        url: \"/DrawHis\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 银行卡列表\r\nexport const BankCardListApi = data => {\r\n    return Service({\r\n        url: \"/BankCardList\",\r\n        method: \"get\",\r\n    })\r\n}\r\n\r\n// 新增银行卡\r\nexport const AddBankCardApi = data => {\r\n    return Service({\r\n        url: \"/AddBankCard\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 删除银行卡\r\nexport const DelBankCardApi = data => {\r\n    return Service({\r\n        url: \"/DelBankCard\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 发送验证码\r\nexport const SendSmsApi = data => {\r\n    return Service({\r\n        url: \"/SendSms\",\r\n        method: \"get\"\r\n    })\r\n}\r\n\r\n// 检验验证码是否正确\r\nexport const checkCodeApi = data => {\r\n    return Service({\r\n        url: \"/checkCode\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 发起提款\r\nexport const withDrawApi = data => {\r\n    return Service({\r\n        url: \"/withDraw\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 代存\r\nexport const agentDepositApi = data => {\r\n    return Service({\r\n        url: \"/agentDeposit\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}\r\n\r\n// 彩金\r\nexport const agentBonusApi = data => {\r\n    return Service({\r\n        url: \"/agentBonus\",\r\n        method: \"post\",\r\n        data\r\n    })\r\n}"]}]}