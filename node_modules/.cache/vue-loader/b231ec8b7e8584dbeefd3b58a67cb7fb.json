 },\r\n        mounted() {\r\n            var has_pay_pwd = localStorage.getItem('has_pay_pwd')\r\n            if(has_pay_pwd == 'true'){\r\n                this.DrawMoneyInfoData()\r\n            }else{\r\n                this.showPay = true\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/finance.css);\r\n</style>"]}]}