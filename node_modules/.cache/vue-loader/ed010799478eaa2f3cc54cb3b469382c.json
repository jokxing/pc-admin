             <li v-for=\"(item, index) in moneyTab\" :key=\"index\" :class=\"{ 'bor_active': num == index }\" @click=\"moneyTabClick(item,index)\">\r\n                    ￥{{ item }}\r\n                </li>\r\n            </ul>\r\n            <el-input v-model=\"money\" placeholder=\"请输入存款金额\" @keyup.native=\"moneyKeyup()\"></el-input>\r\n        </el-row>\r\n\r\n        <el-row>\r\n            <h5>备注</h5>\r\n            <el-input placeholder=\"20个字\"></el-input>\r\n        </el-row>\r\n\r\n        <el-row>\r\n            <h5>支付密码</h5>\r\n            <el-input placeholder=\"请输入支付密码\"></el-input>\r\n            <span class=\"primary-color\" style=\"margin-left: 10px;cursor: pointer;\">忘记密码？</span>\r\n        </el-row>\r\n\r\n        <el-row>\r\n            <el-button class=\"primary-button\" round>确认{{ name }}</el-button>\r\n        </el-row>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        data() {\r\n            return {\r\n                title: \"\",\r\n                name: \"\",\r\n                // tab\r\n                activeName: 'first',\r\n                // 存款选择金额\r\n                moneyTab: ['18','50','100','200','500'],\r\n                num: -1,\r\n                money: \"\",\r\n            }\r\n        },\r\n        methods: {\r\n            // tab\r\n            handleClick(tab, event) {\r\n                if(this.activeName == 'second'){\r\n                    this.$router.push('/ctkRecord')\r\n                }\r\n            },\r\n            // 存款金额-选择金额\r\n            moneyTabClick(item, index) {\r\n                this.num = index;\r\n                this.money = item\r\n            },\r\n            moneyKeyup() {\r\n                this.num = -1\r\n            }\r\n        },\r\n        mounted() {\r\n            if(this.$router.history.current.path == '/bonusGift'){\r\n                this.title = \"彩金赠送\";\r\n                this.name = \"彩金\"\r\n            }else{\r\n                this.title = \"会员代存\";\r\n                this.name = \"代存\"\r\n            }\r\n        },\r\n        watch: {\r\n            $route(to, from){\r\n                if(to.fullPath == '/bonusGift'){\r\n                    this.title = \"彩票赠送\";\r\n                    this.name = \"彩金\"\r\n                }else{\r\n                    this.title = \"会员代存\";\r\n                    this.name = \"代存\"\r\n                }\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/finance.css);\r\n</style>"]}]}