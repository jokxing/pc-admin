e=\"false\"\r\n                            range-separator=\"至\"\r\n                            start-placeholder=\"开始日期\"\r\n                            end-placeholder=\"结束日期\"\r\n                            value-format=\"yyyy-MM-dd\">\r\n                        </el-date-picker>\r\n                    </el-col>\r\n                    <el-col :span=\"7\">\r\n                        <el-input placeholder=\"会员账号\" v-model=\"ruleForm.Name\"></el-input>\r\n                    </el-col>\r\n                    <el-col :span=\"4\">\r\n                        <el-select placeholder=\"全部\" v-model=\"ruleForm.Status\">\r\n                            <el-option label=\"全部\" value=\"\"></el-option>\r\n                            <el-option label=\"审核中\" value=\"0\"></el-option>\r\n                            <el-option label=\"成功\" value=\"1\"></el-option>\r\n                            <el-option label=\"拒绝\" value=\"2\"></el-option>\r\n                        </el-select>\r\n                    </el-col>\r\n                    <el-col :span=\"6\" class=\"form_button\">\r\n                        <el-button class=\"primary-button\" round @click=\"hanldQuery\">查询</el-button>\r\n                        <el-button plain round @click=\"hanldReset('ruleForm')\">重置</el-button>\r\n                    </el-col>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-row>\r\n\r\n        <el-row>\r\n            <el-table\r\n                :data=\"tableData\"\r\n                style=\"width:100%\"\r\n                v-loading=\"tableLoading\">\r\n                    <el-table-column\r\n                        prop=\"created_at\"\r\n                        label=\"日期\"\r\n                        width=\"150\">\r\n                    </el-table-column>    \r\n                    <el-table-column\r\n                        prop=\"post_member_name\"\r\n                        label=\"用户名\"\r\n                        width=\"100\">\r\n                    </el-table-column>    \r\n                    <el-table-column\r\n                        prop=\"before_balance\"\r\n                        label=\"调时余额\"\r\n                        width=\"\">\r\n                    </el-table-column>    \r\n                    <el-table-column\r\n                        prop=\"before_dpt_money\"\r\n                        label=\"调前存款金额\"\r\n                        width=\"120\">\r\n                    </el-table-column>    \r\n                    <el-table-column\r\n                        prop=\"before_dpt_time\"\r\n                        label=\"调前存款次数\"\r\n                        width=\"120\">\r\n                    </el-table-column>    \r\n                    <el-table-column\r\n                        prop=\"before_created_at\"\r\n                        label=\"注册时间\"\r\n                        width=\"150\">\r\n                    </el-table-column>    \r\n                    <el-table-column\r\n                        label=\"状态\"\r\n                        width=\"80\">\r\n                            <template slot-scope=\"scope\">\r\n                                <span class=\"yellow\" v-if=\"scope.row.status == 0\">待审核</span>\r\n                                <span class=\"green\" v-if=\"scope.row.status == 1\">成功</span>\r\n                                <span class=\"red\" v-if=\"scope.row.status == 2\">拒绝</span>\r\n                            </template>\r\n                    </el-table-column>   \r\n                    <el-table-column\r\n                        prop=\"verify_remark\"\r\n                        label=\"备注\"\r\n                        width=\"100\">\r\n                    </el-table-column>   \r\n            </el-table>\r\n        </el-row>\r\n\r\n        <!-- 分页 -->\r\n        <el-pagination\r\n            background\r\n            layout=\"prev, pager, next\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page.sync=\"ruleForm.Page\" \r\n            :total=\"total\"\r\n            :page-size=\"pagesize\">\r\n        </el-pagination>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import { ChangeLineLogsListApi } from '../../http/api.js'\r\n    export default {\r\n        data() {\r\n            var myDate = new Date();\r\n            return {\r\n                tableLoading: true,\r\n                date: [myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\", myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate()],\r\n                ruleForm: {\r\n                    Name: '',\r\n                    Status: \"\",\r\n                    Page: 1,\r\n                    Start: myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\",\r\n                    End: myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate(),\r\n                },\r\n                tableData: [],\r\n                total: 0,\r\n                pagesize: 0\r\n            }\r\n        },\r\n        methods: {\r\n            changeLineLogsListData () {\r\n                var _this = this;\r\n                this.tableLoading = true;\r\n                ChangeLineLogsListApi(_this.ruleForm).then(res => {\r\n                    _this.tableLoading = false;\r\n                    _this.tableData = res.data.message.Data;\r\n                    _this.total = res.data.message.Total;\r\n                    _this.pagesize = res.data.message.Pagesize;\r\n                }).catch(err => {\r\n                    _this.tableLoading = false\r\n                })\r\n            },\r\n            // 查询\r\n            hanldQuery () {\r\n                this.ruleForm.Start = this.date[0];\r\n                this.ruleForm.End = this.date[1];\r\n                this.changeLineLogsListData()\r\n            },\r\n            // 重置\r\n            hanldReset (formName) {\r\n                this.ruleForm.Name = \"\";\r\n                this.ruleForm.Status =\"\";\r\n                var myDate = new Date();\r\n                this.date = [myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-01\", myDate.getFullYear() + \"-\" + (myDate.getMonth() + 1) + \"-\" + myDate.getDate()]\r\n            },\r\n            // 分页\r\n            handleCurrentChange(val) {\r\n                this.ruleForm.Page = val;\r\n                this.changeLineLogsListData()\r\n            }\r\n        },\r\n        mounted () {\r\n            this.changeLineLogsListData();\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    @import url(../../assets/css/management.css);\r\n</style>"]}]}