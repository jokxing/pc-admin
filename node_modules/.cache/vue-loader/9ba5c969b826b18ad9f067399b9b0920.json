n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-col :span=\"7\">\r\n                        <el-date-picker\r\n                            v-model=\"date\"\r\n                            type=\"daterange\"\r\n                            :clearable=\"false\"\r\n                            :picker-options=\"pickerOptions\"\r\n                            range-separator=\"至\"\r\n                            value-format=\"yyyy-MM-dd\"\r\n                            start-placeholder=\"开始日期\"\r\n                            end-placeholder=\"结束日期\">\r\n                        </el-date-picker>\r\n                    </el-col>\r\n                    <el-col :span=\"8\">\r\n                         <el-radio-group v-model=\"tabPosition\" @change=\"tabChange\">\r\n                            <el-radio-button label=\"1\">今日</el-radio-button>\r\n                            <el-radio-button label=\"2\">昨日</el-radio-button>\r\n                            <el-radio-button label=\"3\">7天</el-radio-button>\r\n                            <el-radio-button label=\"4\">30天</el-radio-button>\r\n                        </el-radio-group>\r\n                    </el-col>\r\n                    <el-col :span=\"8\">\r\n                        <el-button round class=\"primary-button\" @click=\"query()\">查询</el-button>\r\n                        <el-button round plain>重置</el-button>\r\n                    </el-col>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-row>\r\n\r\n        <!-- 表格 -->\r\n        <el-row>\r\n            <el-table\r\n                :data=\"tableData\"\r\n                style=\"width: 100%\">\r\n                <el-table-column\r\n                    label=\"比赛类型\"\r\n                    width=\"120\">\r\n                    <template slot-scope=\"scope\">\r\n                        <span class=\"match_type primary-color\" @click=\"matchTypeClick(scope.row.id)\">{{ scope.row.type }}</span>\r\n  